<?xml version="1.0" encoding="UTF-8"?>
<simulation>
  <start_time>0</start_time>
  <end_time>7200</end_time>
  <time_increment>5</time_increment>
  <time_stay_distribution type="Normal">
    <mean>600</mean>
    <sd>60</sd>
  </time_stay_distribution>
  <interval_between_stays_distribution type="Exponential">
    <mean>1800</mean>
  </interval_between_stays_distribution>
  <mno>
    <mno_name>MNO1</mno_name>
    <prob_mobile_phone>0.35</prob_mobile_phone>
  </mno>
  <prob_sec_mobile_phone>0.40</prob_sec_mobile_phone>
  <movement_pattern type="home_work">
    <home>
      <x>2000</x>
      <y>7500</y>
      <x_sd>1000</x_sd>
      <y_sd>1000</y_sd>
    </home>
    <home>
      <x>8000</x>
      <y>7500</y>
      <x_sd>750</x_sd>
      <y_sd>750</y_sd>
    </home>
    <work>
      <x>2500</x>
      <y>8500</y>
      <x_sd>750</x_sd>
      <y_sd>750</y_sd>
    </work>
    <work>
      <x>5000</x>
      <y>2000</y>
      <x_sd>750</x_sd>
      <y_sd>750</y_sd>
    </work>
    <anchor_point>
      <x>8000</x>
      <y>4000</y>
      <x_sd>700</x_sd>
      <y_sd>700</y_sd>
    </anchor_point>
    <anchor_point>
      <x>3000</x>
      <y>5000</y>
      <x_sd>700</x_sd>
      <y_sd>700</y_sd>
    </anchor_point>
    <percent_time_home>0.20</percent_time_home>
    <percent_time_work>0.20</percent_time_work>
    <percent_time_anchor_point>0.20</percent_time_anchor_point>
    <percent_time_travel>0.50</percent_time_travel>
    <direction_angle_distribution type="Laplace">
      <scale>15</scale>
    </direction_angle_distribution>
    <prob_anchor_point>0.8</prob_anchor_point>
  </movement_pattern>
  <connection_type>strength</connection_type>
  <event_type>cellIDTA</event_type>
  <conn_threshold>-85</conn_threshold>
  <grid_file>grid.csv</grid_file>
  <grid_dim_tile_x>0</grid_dim_tile_x>
  <grid_dim_tile_y>0</grid_dim_tile_y>
  <persons_file>persons.csv</persons_file>
  <antennas_file>antennas.csv</antennas_file>
  <random_seed>12</random_seed>
  <output_dir>output2</output_dir>
</simulation>
